{
  "manifest_version": 3,
  "name": "Google Podcast Unplayed",
  "version": "0.0.1",
  "description": "This extension will show you the unplayed episodes of a podcast in Google Podcasts web app",
  "action":{},
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ]
}
